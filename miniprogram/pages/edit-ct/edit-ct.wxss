.container,.title_input_container {
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  box-sizing: border-box;
}

.container {
  flex-direction: column;
  width: 100%;
  height: 100%;
  padding: 30rpx;
}

.title_input_container{
  width: 100%;
  margin-bottom: 30rpx;
}

.btn_container {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
    width:550rpx;
    height:80rpx;
    margin-top: 40rpx;
    background-color: #09bb07;
    border-radius:40rpx;
  }
  
 
  .btn_text {
    flex-shrink: 0;
    font-size:30rpx;
    line-height: 30rpx;
    font-weight:500;
    color:rgba(255,255,255,1);
  }
  

page {
  width: 100%;
  height: 100%;
  background-color: #ffffff;
}
