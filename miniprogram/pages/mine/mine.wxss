.container,
.content_container,
.des_history_container {
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  background-color: #ffffff;
}

.container {
  flex-direction: column;
  width: 100%;
}

.header_container {
  display: flex;
  align-items: center;
  box-sizing: border-box;
  justify-content: center;
  flex-direction: column;
  flex-grow: 1;
  height: 350rpx;
  padding-bottom: 16rpx;
}

.content_container {
  flex-direction: column;
  width: 100%;
  padding: 0 60rpx;
}

.header_img {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  flex-shrink: 0;
  margin-bottom: 30rpx;
  background-color: #F6F6F6;
  background-size: 150rpx 150rpx;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');

}

.header_text {
  line-height: 36rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  flex-shrink: 0;
}

.user_type{
  margin-top: 20rpx;
  line-height: 36rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: #09bb07;
  flex-shrink: 0;
}

.line {
  width: 100%;
  height: 20rpx;
  background-color: #f6f6f6;
}

.des_history_container {
  width: 100%;
  flex-direction: row;
  height: 120rpx;
}

.history_icon {
  width: 40rpx;
  height: 28rpx;
  flex-shrink: 0;
  margin-right: 32rpx;
}

.my_icon {
  width: 40rpx;
  height: 38rpx;
  flex-shrink: 0;
  margin-right: 32rpx;
}

.history_text {
  line-height: 32rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  flex-grow: 1;
}

.history_arrow_icon {
  width: 16rpx;
  height: 32rpx;
  flex-shrink: 0;
}

.line1 {
  width: 100%;
  height: 1rpx;
  background: rgba(246, 246, 246, 1);
}


page {
  width: 100%;
  height: 100%;
  background-color: #F6F6F6;
}