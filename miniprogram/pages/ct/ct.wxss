.container, .top_container, .lm_title_container{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  box-sizing: border-box;
}

.container {
  flex-direction: column;
  width: 100%;
  height: 100%;
}

.top_container {
  flex-direction: column;
  width: 100%;
  background: #ffffff;
  flex-shrink: 0;
  padding: 20rpx 20rpx 0 20rpx;
  margin-bottom: 10rpx;
}

.title {
  width: 100%;
  color: #333333;
  font-size: 30rpx;
  margin-bottom: 25rpx;
  flex-shrink: 0;
  text-align: left;
}

.des {
  width: 100%;
  color: #999999;
  font-size: 30rpx;
  margin-bottom: 20rpx;
  flex-shrink: 0;
  text-align: left;
}


.lm_title_container {
  flex-direction: row;
  width: 100%;
  background: #ffffff;
  flex-shrink: 0;
  height: 100rpx;
  padding: 0 20rpx;
}

.lm_title {
  font-size: 32rpx;
  text-align: left;
  flex-grow: 1;
  color: #999999;
}

.create_lm {
  font-size: 32rpx;
  flex-shrink: 0;
  color: #09bb07;
}


.create {
  color: #09bb07;
  font-size: 28rpx;
  flex-grow: 1;
  text-align: right;
}

.title_m {
  width: 100%;
  color: #333333;
  font-size: 26rpx;
  margin-bottom: 20rpx;
  text-align: left;
  flex-shrink: 0;
}

.list_container {
  width: 100%;
  margin-top: 2rpx;
  margin-bottom: 90rpx;
}


.empty_container {
  position: absolute;
  display: flex;
  flex-direction: column;
  background-color: #ffffff;
  justify-content: center;
  align-items: center;
  margin: 0;
  padding: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  box-sizing: border-box;
}

.empty_image {
  width: 178rpx;
  height: 180rpx;
  margin-top: -90rpx;
  margin-bottom: 40rpx;
}

.empty_text {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(187, 187, 187, 1);
  line-height: 24rpx;
}


page {
  width: 100%;
  height: 100%;
  background-color: #f6f6f6;
}
