.container ,.lm_title_container{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  box-sizing: border-box;
}

.container {
  flex-direction: column;
  width: 100%;
  height: 100%;
}

.title {
  display: flex;
  width: 100%;
  color: #333;
  font-size: 36rpx;
  padding: 30rpx;
  flex-shrink: 0;
  text-align: left;
  margin-left:30rpx;
}

.content {
  width: 100%;
  height: 250rpx;
  background-color: #ffffff;
  box-sizing: border-box;
  padding: 30rpx;
  font-size: 30rpx;
  flex-shrink: 0;
  color: #666;
}

.btn_container {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  flex-shrink: 0;
  width: 550rpx;
  height: 80rpx;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
  background-color: #09bb07;
  border-radius: 40rpx;
}

.btn_text {
  flex-shrink: 0;
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}


.lm_title_container {
  flex-direction: row;
  width: 100%;
  background: #ffffff;
  flex-shrink: 0;
  height: 100rpx;
  padding: 0 20rpx;
}

.lm_title {
  font-size: 32rpx;
  text-align: left;
  flex-grow: 1;
  color: #999999;
}

.create_lm {
  font-size: 32rpx;
  flex-shrink: 0;
  color: #09bb07;
}


.create {
  color: #09bb07;
  font-size: 28rpx;
  flex-grow: 1;
  text-align: right;
}

.title_m {
  width: 100%;
  color: #333333;
  font-size: 26rpx;
  margin-bottom: 20rpx;
  text-align: left;
  flex-shrink: 0;
}

.list_container {
  width: 100%;
  margin-top: 2rpx;
  margin-bottom: 90rpx;
}


page {
  width: 100%;
  height: 100%;
  background-color: #f6f6f6;
}
