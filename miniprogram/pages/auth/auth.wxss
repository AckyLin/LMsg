.container {
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    background-color: #FFFFFF;
}

.container {
    flex-direction: column;
    width: 100%;
}

.header_container {
    display: flex;
    align-items: center;
    box-sizing: border-box;
    justify-content: center;
    flex-direction: column;
    flex-grow: 1;
    height: 250rpx;
    margin-top: 250rpx;
    margin-bottom: 260rpx;
}

.content_container {
    flex-direction: column;
    width: 100%;
    padding: 0 60rpx;
}

.header_img {
    width: 180rpx;
    height: 180rpx;
    flex-shrink: 0;
    margin-bottom: 20rpx;
}

.header_text {
    font-size:36rpx;
    font-weight:500;
    color:rgba(51,51,51,1);
}


.btn_container {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
    width:550rpx;
    height:100rpx;
    background:rgba(22,185,129,1);
    border-radius:50rpx;
  }
  

  
  .btn_icon{
    width:36rpx;
    height:28rpx;
    flex-shrink: 0;
    margin-right: 22rpx;
  }
  
 
  .btn_text {
    flex-shrink: 0;
    font-size:30rpx;
    line-height: 30rpx;
    font-weight:500;
    color:rgba(255,255,255,1);
  }
  
  .des{
    width: 100%;
    text-align: center;
    margin-top: 28rpx;
    line-height: 24rpx;
    font-size:24rpx;
    font-weight:400;
    color:rgba(153,153,153,1);
  }



page {
    width: 100%;
    height: 100%;
    background-color: #FFFFFF;
}